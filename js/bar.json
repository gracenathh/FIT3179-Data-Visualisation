{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title" : {"text": "Ownership Type Proportion"},
    "width": 200,
    "height": 300,
    "data": {
        "url": "https://raw.githubusercontent.com/gracenathh/FIT3179-Data-Visualisation/main/data/ownershiptype.csv"
    },
