{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Literacy Rate x People Having Phone x Number of Sbux",
    "data": {
        "url": "https://raw.githubusercontent.com/gracenathh/FIT3179-Data-Visualisation/main/data/combined-processed.csv",
        "format": {"type": "csv",
                    "parse": {"Phones/1K": "number","Literacy" : "number"}}
    },"width": 750,
    "height": 500,
    "transform": [
        {"filter": "datum.TotalStore > 0"}
    ],
    "mark":"rect",
    "encoding": {
        "x" : {
            "field" : "Phones/1K", "type": "ordinal"
        },
        "y" : {
            "field" : "Literacy", "type":"ordinal"
        },
        "color":{
            "field" : "TotalStore",
            "aggregate" : "max",
            "type": "quantitative",
            "legend" : {
                "title" : null
            }
        },
        "tooltip" : [
            {"field": "Literacy", "type": "ordinal"},
            {"field": "Phones/1K", "type": "ordinal"},
            {"field": "Country", "type": "nominal"}
        ]
    }
}